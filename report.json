{
      "reports": [
        {
          "targets": [
            {
              "name": "teams",
              "inputs": {
                "url": "{TEAMS_URL}",
                "publish": "test-summary-slim"
              },
              "extensions": [
                {
                  "name": "hyperlinks",
                  "inputs": {
                    "links": [
                      {
                        "text": "Logs",
                        "url": "{BUILD_URL}"
                      }
                    ]
                  }
                }
              ]
            }
          ],
          "results": [
            {
              "type": "testng",
              "files": [
                "./results.xml"
              ]
            }
          ]
        }
      ]
    }